{
  "hosting": {
    "redirects":[
      {
        "source":"https://www.karbelcelik.com/",
        "destination":"https://karbelcelik.com/",
        "type":"301"
      }
    ],
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
